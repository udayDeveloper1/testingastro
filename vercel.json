{
  "headers": [
    {
      "source": "/(.*)\\.(js|css|woff2|png|jpg|svg|webp|ttf|otf)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
    "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
